<apex:page controller="Contactlistcontroller" >
  <apex:form >
      <apex:pageBlock title=" Contact List">
          <apex:pageBlockTable value="{!contacts}" var="ct">
              <apex:column value="{!ct.FirstName}"/>
              <apex:column value="{!ct.LastName}"/>
          </apex:pageBlockTable>
      </apex:pageBlock>
    </apex:form>
</apex:page>